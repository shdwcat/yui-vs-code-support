{
    "name": "yui-vs-code-support",
    "displayName": "YUI VS Code Support",
    "description": "Syntax highlighting for YUI files",
    "version": "0.0.1",
    "preview": true,
    "publisher": "shdwcat",
    "author": { "name": "shdwcat" },
    "license": "SEE LICENSE IN LICENSE.txt",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "homepage": "https://github.com/shdwcat/yui-vs-code-support",
    "repository": {
        "type": "git",
        "url": "https://github.com/shdwcat/yui-vs-code-support.git"
    },
    "bugs": {
        "url": "https://github.com/shdwcat/yui-vs-code-support/issues"
    },
    "keywords": ["yui", "gms2"],
    "contributes": {
        "languages": [{
            "id": "yui",
            "aliases": ["yui", "yui"],
            "extensions": [".yui"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "yui",
            "scopeName": "source.yui",
            "path": "./syntaxes/yui.tmLanguage.json",
            "embeddedLanguages": {
                "source.yuiscript": "talescript",
                "meta.embedded.yuiscript": "talescript",
                "meta.embedded.inline.yuiscript": "talescript"
            }
        }]
    },
    "scripts": {
        "vscode:prepublish": "npm run convert",
        "convert": "js-yaml syntaxes/yui.tmLanguage.yaml > syntaxes/yui.tmLanguage.json"
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}